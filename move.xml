<tryton>
    <data>
        <record model="ir.ui.view" id="move_view_form">
            <field name="model">stock.move</field>
            <field name="inherit" ref="stock.move_view_form"/>
            <field name="arch" type="xml">
                <![CDATA[
                <data>
                    <xpath
                        expr="/form/field[@name=&quot;unit_price&quot;]"
                        position="after">
                        <newline/>
                        <label name="amount"/>
                        <field name="amount"/>
                    </xpath>
                </data>
                ]]>
            </field>
        </record>
    </data>
</tryton>
